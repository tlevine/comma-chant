OLDPS1="$PS1"
# PS1='\033[32mcsv \033[35m>>>\033[39m '
PS1='csv >>> '
alias l='./when-i-say "carriage return" "line feed" 2'
alias c='./when-i-say value comma 2'

deactivate() {
  PS1="$OLDPS1"
  unalias l
  unalias c
}

clear
# printf '\033[32mCommands\033[39m\n'
echo Commands
echo '  l : CR     LF  [rate]'
echo '  c : value  ,   [rate]'
